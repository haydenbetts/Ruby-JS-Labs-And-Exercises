<!-- name  -->
<h2> <%= @figure.name %> </h2>

<!-- landmark list  -->
<h3> Figure Landmarks </h3>

<ul>
<% @figure.landmarks.each do |landmark| %>
  <li><%= landmark.name %></li>
<% end %>
</ul>

<!-- title list  -->
<h3> Figure Titles </h3>

<ul>
<% @figure.titles.each do |title| %>
  <li><%= title.name %></li>
<% end %>
</ul>
